{
  "name": "cxm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k \"yarn dev:web\" \"yarn dev:proxy\" \"yarn dev:api\" \"yarn dev:app\"",
    "dev:web": "NODE_OPTIONS='--inspect' next dev --port=3001",
    "dev:proxy": "node _scripts/development/proxy.js",
    "dev:app": "cd app && yarn dev",
    "dev:api": "cd api && yarn dev",
    "docs": "swagger-jsdoc -d swagger.js src/pages/api/public/**/*.yaml -o public/docs/api.json",
    "codegen": "graphql-codegen --config codegen.js --watch",
    "codegen:build": "graphql-codegen --config codegen.js",
    "build:translate": "./_scripts/translate/run.sh",
    "deploy:root": "./_scripts/deploy/run.sh _root_",
    "deploy:admin": "./_scripts/deploy/run.sh admin",
    "deploy:api": "cd api && yarn deploy",
    "deploy:docs": "./_scripts/deploy/run.sh docs",
    "deploy:uploads": "./_scripts/deploy/run.sh uploads",
    "build:root": "./_scripts/build/run.sh _root_",
    "build:admin": "./_scripts/build/run.sh admin",
    "build:api": "./_scripts/build/run.sh api",
    "build:docs": "./_scripts/build/run.sh docs",
    "build:uploads": "./_scripts/build/run.sh uploads"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@auth0/auth0-spa-js": "^1.13.6",
    "@auth0/nextjs-auth0": "^1.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.2",
    "@svgr/webpack": "^5.5.0",
    "array-move": "^3.0.1",
    "aws-sdk": "^2.852.0",
    "axios": "^0.21.1",
    "cloudinary-react": "^1.6.8",
    "dayjs": "^1.10.3",
    "deep-object-diff": "^1.1.0",
    "graphql": "^15.4.0",
    "i18next": "^19.8.9",
    "immer": "^8.0.0",
    "is-electron": "^2.2.0",
    "lodash": "^4.17.20",
    "mobx": "^6.0.4",
    "next": "^10.0.7",
    "next-i18next": "8.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-sortable-hoc": "^1.11.0",
    "react-tooltip": "^4.2.11",
    "react-transition-group": "^4.4.1",
    "redoc": "^2.0.0-rc.45",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "sass": "^1.32.2",
    "subscriptions-transport-ws": "^0.9.18",
    "unsplash-js": "^7.0.4",
    "xss": "^1.0.8"
