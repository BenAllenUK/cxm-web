{
  "name": "omnea",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "setup": "run-s setup:*:*; husky install",
    "build": "DEPLOY_ENVIRONMENT=/prod/ yarn envars; . ./dev/env/.envars; run-s build:*:*",
    "start": "run-p start:*:*",
    "deploy": "DEPLOY_ENVIRONMENT=/prod/ yarn envars; . ./dev/env/.envars; run-s build:libs:*; run-s deploy:*:*",
    "lint": "run-s lint:*:*",
    "test": "run-s test:*:*",
    "clean": "run-s clean:*:*",
    "envars": "yarn --cwd dev/env start;",
    "setup:dev:env": "yarn --cwd dev/env setup",
    "start:web": "run-p start:clients:web start:clients:proxy start:services:api",
    "start:nextjs": "run-p start:libs:queries start:libs:nextjs",
    "setup:libs:queries": "yarn --cwd libs/queries setup",
    "setup:libs:nextjs": "yarn --cwd libs/nextjs setup",
    "setup:clients:proxy": "yarn --cwd clients/proxy setup",
    "setup:clients:app": "yarn --cwd clients/app setup",
    "setup:clients:web": "yarn --cwd clients/web setup",
    "setup:services:api": "yarn --cwd services/api setup",
    "build:libs:queries": "yarn --cwd libs/queries build",
    "build:libs:nextjs": "yarn --cwd libs/nextjs build",
    "build:clients:app": "yarn --cwd clients/app build",
    "build:clients:proxy": "yarn --cwd clients/proxy build",
    "build:clients:web": "yarn --cwd clients/web build",
    "build:services:api": "yarn --cwd services/api build",
    "start:libs:queries": "yarn --cwd libs/queries start",
    "start:libs:nextjs": "yarn --cwd libs/nextjs start",
    "start:clients:app": "yarn --cwd clients/app start",
    "start:clients:proxy": "yarn --cwd clients/proxy start",
    "start:clients:web": "yarn --cwd clients/web start",
    "start:services:api": "yarn --cwd services/api start",
    "deploy:libs:queries": "yarn --cwd libs/queries deploy",
    "deploy:libs:nextjs": "yarn --cwd libs/nextjs deploy",
    "deploy:clients:app": "yarn --cwd clients/app deploy",
    "deploy:clients:proxy": "yarn --cwd clients/proxy deploy",
    "deploy:clients:web": "yarn --cwd clients/web deploy",
    "deploy:services:api": "yarn --cwd services/api deploy",
    "lint:libs:queries": "yarn --cwd libs/queries lint",
    "lint:libs:nextjs": "yarn --cwd libs/nextjs lint",
    "lint:clients:app": "yarn --cwd clients/app lint",
    "lint:clients:proxy": "yarn --cwd clients/proxy lint",
    "lint:clients:web": "yarn --cwd clients/web lint",
    "lint:services:api": "yarn --cwd services/api lint",
    "test:libs:queries": "yarn --cwd libs/queries test",
    "test:libs:nextjs": "yarn --cwd libs/nextjs test",
    "test:clients:app": "yarn --cwd clients/app test",
    "test:clients:proxy": "yarn --cwd clients/proxy test",
    "test:clients:web": "yarn --cwd clients/web test",
    "test:services:api": "yarn --cwd services/api test",
    "clean:libs:queries": "yarn --cwd libs/queries clean",
    "clean:libs:nextjs": "yarn --cwd libs/nextjs clean",
    "clean:clients:app": "yarn --cwd clients/app clean",
    "clean:clients:proxy": "yarn --cwd clients/proxy clean",
    "clean:clients:web": "yarn --cwd clients/web clean",
    "clean:services:api": "yarn --cwd services/api clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BenAllenUK/cxm-web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BenAllenUK/cxm-web/issues"
  },
  "homepage": "https://github.com/BenAllenUK/cxm-web#readme",
  "dependencies": {},
  "devDependencies": {
    "eslint": "^7.24.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "husky": "^6.0.0"
  }
}
